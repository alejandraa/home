# Load global AWS settings which are not dependent on the user's account
# settings.

export EC2_HOME=$HOME/.ec2
export AWS_ELB_HOME=$EC2_HOME
export PATH=$PATH:$EC2_HOME/bin
export JAVA_HOME=/System/Library/Frameworks/JavaVM.framework/Home/

# Load un-tracked AWS settings dependent on client and machine.

source $HOME/.dots/config/aws.zsh
